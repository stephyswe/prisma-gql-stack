{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "target": "es2019",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true
  },
  "include": [
    "prisma/*.ts",
    "src/**/*"
  ]  
}